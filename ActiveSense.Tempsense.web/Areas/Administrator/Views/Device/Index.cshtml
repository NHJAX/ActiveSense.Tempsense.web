@model IEnumerable<ActiveSense.Tempsense.model.Model.devices>

<h2>
    <font color="#999">
 Devices
    </font>
</h2>
<hr />
<p>
    @Html.ActionLink("create", "Create", new { }, new { @class = "btn btn-primary"})
</p>
<div class="table-responsive">
    <table class="table table-striped table-bordered" style="padding-bottom: 60px;">
        <thead>
            <tr class="active">
                <th>
                    Company
                    @*@Html.DisplayNameFor(model => model.Company.Name)*@
                </th>
                <th>
                    device
                    @*@Html.DisplayNameFor(model => model.Name)*@
                </th>
                @*<th>
                    @Html.DisplayNameFor(model => model.Clavedevice)
                </th>*@
                device Active
                @*<th>
                    @Html.DisplayNameFor(model => model.Active)
                </th>*@
                <th>
                    Options
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.company.Name)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.name)
                    </td>
                    @*<td>
                        @Html.DisplayFor(modelItem => item.Clavedevice)
                    </td>*@
                    @*<td>
                        @Html.DisplayFor(modelItem => item.Active)
                    </td>*@
                    <td>
                        @Html.ActionLink("Edit", "Edit", new { id = item.DeviceID }, new { @class = "btn btn-primary btn-xs" })

                        @Html.ActionLink("Eliminate", "Delete", new { id = item.DeviceID }, new { @class = "btn btn-primary btn-xs" })
                    </td>
                </tr>
            }
        </tbody>
    </table>
    </div>